{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/fep-core.schema.json",
  "title": "Core FEP Dataset",
  "type": "object",
  "properties": {
    "version": {
      "type": "string"
    },
    "features": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "events": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "processes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "version",
    "features",
    "events",
    "processes"
  ]
}